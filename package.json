{
  "name": "asset-management-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev src",
    "build": "next build src",
    "start": "next start src",
    "lint": "next lint",
    "style:lint": "eslint --fix './src/**/*.{js,jsx,ts,tsx}'",
    "style:check": "prettier --check './src/**/*.{js,jsx,ts,tsx}'",
    "style:write": "prettier --write './src/**/*.{js,jsx,ts,tsx}'",
    "build-public": "cpx \"src/public/**/*.*\" \"public\" -C",
    "test": "eslint --fix -f table './src/**/*.ts' './src/**/*.tsx'",
    "pretty-quick": "pretty-quick",
    "lint-staged": "lint-staged",
    "prepare": "husky install"
  },
  "dependencies": {
    "chart.js": "2.9.1",
    "chartjs-plugin-colorschemes": "^0.4.0",
    "chartjs-plugin-datalabels": "0.5.0",
    "chartjs-plugin-doughnutlabel": "2.0.3",
    "chartjs-plugin-labels": "^1.1.0",
    "chartjs-plugin-piechart-outlabels": "^0.1.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "googleapis": "^100.0.0",
    "next": "12.1.6",
    "react": "18.1.0",
    "react-chartjs-2": "2.8.0",
    "react-dom": "18.1.0",
    "react-icons": "^4.3.1",
    "react-query": "^3.38.1"
  },
  "devDependencies": {
    "@types/chartjs-plugin-colorschemes": "^0.4.1",
    "@types/node": "17.0.31",
    "@types/react": "18.0.8",
    "@types/react-dom": "18.0.3",
    "@types/react-query": "^1.2.9",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "cpx": "^1.5.0",
    "eslint": "^8.14.0",
    "eslint-config-next": "^12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-formatter-table": "^7.32.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.4.1",
    "prettier": "^2.6.2",
    "react-query-devtools": "^2.6.3",
    "sass": "^1.51.0",
    "ts-loader": "^9.3.0",
    "typescript": "4.6.4",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix -f table"
    ]
  }
}
